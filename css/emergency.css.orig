/* enable absolute positioning */
.inner-addon {
    position: relative;
}

/* style icon */
.inner-addon .icon {
    position: absolute;
    padding: 10px;
    pointer-events: none;
    z-index: 10;
    margin-top: 7.5px;
    margin-right: 5px;
    color: darkgray;
}

/* align icon */
.left-addon .icon {
    left: 0px;
}

.right-addon .icon {
    right: 0px;
}

